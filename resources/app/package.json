{
  "name": "epam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run export-configs; vue-cli-service serve",
    "build": "rm -rf ../../../public/assets/app/{js,css,img} && vue-cli-service build --no-clean",
    "watch": "rm -rf ../../../public/assets/app/{js,css,img} && vue-cli-service build --no-clean --watch",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "npm run export-configs; vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "export-configs": "cd ../../ && php artisan config:export && cd -"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^27.1.0",
    "@ckeditor/ckeditor5-vue": "^2.0.1",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dompurify": "^2.3.3",
    "js-yaml": "^4.0.0",
    "marked": "^3.0.8",
    "postcss": "^7.0.35",
    "register-service-worker": "^1.7.1",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vue3-popper": "^1.4.0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-utils": "^25.2.1",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^25.2.1",
    "@tailwindcss/postcss7-compat": "^2",
    "@testing-library/vue": "^6.4.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "axios-mock-adapter": "^1.20.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2"
  }
}
